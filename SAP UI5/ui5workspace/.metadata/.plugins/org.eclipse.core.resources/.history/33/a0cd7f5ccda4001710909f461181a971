package test01;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class ArrayEx01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int test_case = 0; test_case < T; test_case++) {
			int n = sc.nextInt();
			int m = sc.nextInt();
			int room[][] = new int[m][n];

			for (int i = 0; i < m; i++) {
				for (int j = 0; j < n; i++) {
					room[m][n] = 0;
				}
			}

			for (int i = 0; i < n; i++) {
				int cnt = sc.nextInt();
				for (int j = m - 1; j < cnt; j++) {
					room[i][j] = 1;
				}
			}

			int count[][] = new int[m][n];

			for (int i = 0; i < m; i++) {
				for (int j = 0; j < n; i++) {
					if (room[i][j] == 1) {
						int c = 1;
						for (int k = 0; k < n; k++) {
							if (room[m][k] == 0) {
								count[i][j] = +1;
							}
						}
					}
				}
			}

			int max = 0;
			for (int i = 0; i < m; i++) {
				for (int j = 0; j < n; i++) {
					if (max < room[m][n]) {
						max = room[m][n];
					}
				}
			}

			System.out.print("#" + (test_case + 1) + " ");
		}
	}

}
