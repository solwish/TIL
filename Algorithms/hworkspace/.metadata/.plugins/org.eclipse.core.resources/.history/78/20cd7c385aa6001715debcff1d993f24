import java.util.Scanner;

public class SelectionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int[] array = new int[10];
		int[] cnt = new int[10];
		int temp = 0;

		for (int i = 0; i < array.length; i++) {
			array[i] = sc.nextInt();
		}
		for (int i = 0; i < array.length; i++) {
			cnt[i] = array[i];
		}

		for (int i = array.length - 1; i > 0; i--) {
			for (int j = i - 1; j >= 0; j--) {
				if (array[i] < array[j]) {
					temp = array[i];
					array[i] = array[j];
					array[j] = temp;
				}
			}
		}

		for (int i = 0; i < cnt.length; i++) {
			if (cnt[i] == array[9] || cnt[i] == array[8]) {
				System.out.print("A+ ");
			} else {
				switch (cnt[i]) {
				case array[7]:
					System.out.print("A ");
					break;
				case array[6]:
					System.out.print("B+ ");
					break;
				case array[5]:
					System.out.print("B ");
					break;
				case array[4]:
					System.out.print("C+ ");
					break;
				case array[3]:
					System.out.print("C ");
					break;
				case array[2]:
					System.out.print("D+ ");
					break;
				case array[1]:
					System.out.print("D ");
					break;
				case array[0]:
					System.out.print("F ");
					break;

				default:
					break;
				}
			}

		}
	}
}
