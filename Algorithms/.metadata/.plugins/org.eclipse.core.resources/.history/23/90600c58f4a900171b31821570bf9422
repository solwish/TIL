package swExpert;

import java.util.Scanner;

public class Snail {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int test_case = 0; test_case < T; test_case++) {
			int num = sc.nextInt();
			int[][] A = new int[num][num];
			int tg = 1;
			int j = 0;
			int n = 0;

			for (int i = 0; i < num; i++) {
				while (0 <= j && j < 4) {
					A[i][j] = n++;
					j = j + tg;
				}
			}

			System.out.println("#" + (test_case + 1));

			System.out.println();
		}
	}
}
