import java.util.Scanner;

public class SelectionSort {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int[] array = new int[10];
		int[] cnt = new int[10];
		int temp = 0;
		int k = 1;

		for (int i = 0; i < array.length; i++) {
			array[i] = sc.nextInt();
		}

		for (int i = array.length; i <= 1; i--) {
			for (int j = i - 1; j <= 0; j--) {
				if (array[i] < array[j]) {
					temp = array[i];
					array[i] = array[j];
					array[j] = temp;
					cnt[j] = k;
					k++;
				} else {
					cnt[i] = k;
					k++;
				}
			}
		}

		for (int i = 0; i < cnt.length; i++) {
			System.out.print(cnt[i]);
		}
		System.out.println();
		for (int i = 0; i < cnt.length; i++) {
			System.out.print(array[i]);
		}

		for (int i = 0; i < cnt.length; i++) {
			if (cnt[i] <= 2) {
				System.out.print("A+ ");
			} else {
				switch (cnt[i]) {
				case 3:
					System.out.print("A ");
					break;
				case 4:
					System.out.print("B+ ");
					break;
				case 5:
					System.out.print("B ");
					break;
				case 6:
					System.out.print("C+ ");
					break;
				case 7:
					System.out.print("C ");
					break;
				case 8:
					System.out.print("D+ ");
					break;
				case 9:
					System.out.print("D ");
					break;
				case 10:
					System.out.print("F ");
					break;

				default:
					break;
				}
			}
		}
	}

}
