package swExpert;

import java.util.Scanner;

public class MaximumPathLength {
	static int T;
	static int N;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		N = sc.nextInt();
		int M = sc.nextInt();
		int[][] d = new int[N][N];
		int a, b;

		for (int i = 0; i < M; i++) {
			a = sc.nextInt();
			b = sc.nextInt();
			d[a][b] = 1;
			d[b][a] = 1;
		}

		int max = 0;
		for (int s = 0; s < N; s++) {
			int[] pie = new int[N];
			pie[s] = 1;
			dfs(d, s, pie);
		}

	}

	private static void dfs(int[][] d, int v, int[] pie) {
		System.out.println(v);
		for (int k = 0; k < N; k++) {
			if (pie[k] == 0 && d[v][k] == 1) {
				pie[k] = pie[v] + 1;
				dfs(d, v, pie);
			}
		}
	}

}
