package swExpert;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.HashMap;
import java.util.StringTokenizer;

public class JejuTripPlan {

	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int T = Integer.parseInt(br.readLine());

		for (int test_case = 1; test_case <= T; test_case++) {
			StringTokenizer s;
			s = new StringTokenizer(br.readLine());
			int N = Integer.parseInt(s.nextToken());
			int M = Integer.parseInt(s.nextToken());
			int[][] d = new int[N][N];
			PointInfo[] pointInfo = new PointInfo[N];
			// HashMap<Integer, PointInfo> pointInfo;

			for (int i = 0; i < N - 1; i++) {
				s = new StringTokenizer(br.readLine());
				for (int j = i + 1; j < N; j++) {
					d[i][j] = Integer.parseInt(s.nextToken());
					d[j][i] = d[i][j];
				}
			}

			for (int i = 0; i < N; i++) {
				s = new StringTokenizer(br.readLine());
				char check = s.nextToken().charAt(0);
				if (check == 'P') {
					pointInfo[i].pointType = check;
					pointInfo[i].sTime = Integer.parseInt(s.nextToken());
					pointInfo[i].satis = Integer.parseInt(s.nextToken());
				} else
					pointInfo[i].pointType = check;
			}

			for (int i = 0; i < N; i++) {
				for (int j = 0; j < N; j++) {
					System.out.print(d[i][j] + " ");
				}
				System.out.println();
			}
			for (int i = 0; i < N; i++) {
				System.out.println(pointInfo.get(i));
			}

		}
	}

	public class PointInfo {
		char pointType;
		int sTime;
		int satis;

		public PointInfo(char pointType, int sTime, int satis) {
			this.pointType = pointType;
			this.sTime = sTime;
			this.satis = satis;
		}

		public char getPointType() {
			return pointType;
		}

		public void setPointType(char pointType) {
			this.pointType = pointType;
		}

		public int getsTime() {
			return sTime;
		}

		public void setsTime(int sTime) {
			this.sTime = sTime;
		}

		public int getSatis() {
			return satis;
		}

		public void setSatis(int satis) {
			this.satis = satis;
		}

		@Override
		public String toString() {
			return "pointInfo [pointType=" + pointType + ", sTime=" + sTime + ", satis=" + satis + "]";
		}
	}
}
