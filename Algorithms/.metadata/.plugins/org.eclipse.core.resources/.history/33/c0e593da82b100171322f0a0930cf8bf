package swExpert;

import java.util.Scanner;

public class MaximumPathLength {
	static int T;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		T = sc.nextInt();
		int N = sc.nextInt();
		int M = sc.nextInt();
		int[][] d = new int[N][N];
		int a, b;

		for (int i = 0; i < M; i++) {
			a = sc.nextInt();
			b = sc.nextInt();
			d[a][b] = 1;
			d[b][a] = 1;

		}
	}

}
