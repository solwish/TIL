package swExpert;

import java.util.Scanner;

public class HsProgramCheck {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int test_case = 1; test_case <= T; test_case++) {
			int n = sc.nextInt();
			int m = sc.nextInt();
			sc.nextLine();
			String s;
			char[][] C = new char[n][m];

			for (int i = 0; i < n; i++) {
				s = sc.nextLine();
				for (int j = 0; j < m; j++) {
					C[i][j] = s.charAt(j);
				}
			}

			int cursor_l = 0;
			int cursor_h = 0;
			boolean flag = true;

			while (flag) {
				switch (C[cursor_h][cursor_l]) {
				case '<':
					cursor_l -= 1;
					break;

				default:
					break;
				}
			}

			System.out.println("#" + test_case + " ");
		}
	}

}
