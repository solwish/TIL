package swExpert;

import java.util.Scanner;

public class Snail {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int test_case = 0; test_case < T; test_case++) {
			int num = sc.nextInt();
			int[][] A = new int[num][num];
			int tg = 1;
			int i = 0, j = 0, k = num;
			int n = 0;

			while (true) {
				for (int p = 0; p < k; k++) {
					A[i][j] = ++n;
					i++;
					j = j + tg;
				}
				if (k == num - 1)
					break;

			}

			System.out.println("#" + (test_case + 1));

			System.out.println();
		}
	}
}
