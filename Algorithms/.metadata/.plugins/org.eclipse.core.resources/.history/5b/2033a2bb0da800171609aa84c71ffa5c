
public class PracticeSort02 {

	public static void main(String[] args) {

	}

	void mergeSort(int[] A, int p, int r) {
		int q = (p + r) / 2;
		if (p < r) {
			mergeSort(A, p, q);
			mergeSort(A, q + 1, r);
			merge(A, p, q, r);
		}
	}

	void merge(int[] data, int p, int q, int r) {
		int i = p, j = q + 1, k = p;
	}
}
