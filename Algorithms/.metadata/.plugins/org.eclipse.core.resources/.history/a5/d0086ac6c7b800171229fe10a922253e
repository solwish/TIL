package swExpert;

import java.util.Scanner;

public class MillionaireProject {
	static int N;
	static int[] A, B;
	static int ans;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int t_case = 1; t_case <= T; t_case++) {
			N = sc.nextInt();
			A = new int[N];
			B = new int[N];

			for (int i = 0; i < N; i++)
				A[i] = sc.nextInt();

			boolean flag = false;
			for (int i = 0; i < N - 1; i++) {
				if (A[i] < A[i + 1]) {
					B[i + 1] = 1;
					flag = true;
				}
			}

			ans = 0;
			int cnt = 0;
			if (flag)
				for (int i = 0; i < N; i++) {
					if (B[i] == 0) {
						ans -= A[i];
						cnt++;
					} else if (B[i] == 1) {
						ans += cnt * A[i];
						cnt = 0;
					}
				}

			System.out.println("#" + t_case + " " + ans);
		}
	}

}
