package swExpert;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

class MicroXY {
	int x, y, num, direct;

	public MicroXY(int x, int y, int num, int direct) {
		this.x = x;
		this.y = y;
		this.num = num;
		this.direct = direct;
	}

	@Override
	public String toString() {
		return "MicroXY [x=" + x + ", y=" + y + ", num=" + num + ", direct=" + direct + "]";
	}
}

