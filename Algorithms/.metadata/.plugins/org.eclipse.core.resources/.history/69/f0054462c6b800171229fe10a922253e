package swExpert;

import java.util.Scanner;

public class MillionaireProject {
	static int N;
	static int[] A, B;

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();

		for (int t_case = 1; t_case <= T; t_case++) {
			N = sc.nextInt();
			A = new int[N];
			B = new int[N];

			for (int i = 0; i < N; i++)
				A[i] = sc.nextInt();

			for (int i = 0; i < N; i++) {
				if (A[i] < A[i + 1])
					B[i + 1] = 1;
			}
		}

	}

}
