
public class DynamicEX01 {
	static int[] f = new int[500];

	public static void main(String[] args) {
		System.out.println(fib(100));
	}

	static int fib(int n) {
		if (n == 1 || n == 2)
			return 1;
		else if (f[n] > -1) // 배열 f가 -1으로 초기화되어 있다고 가정
			return f[n]; // 즉 이미 계산된 값이라는 의미
		else {
			f[n] = fib(n - 2) + fib(n - 1);
			return f[n];
		}
	}

}
