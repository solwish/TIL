<h1>Posts#edit</h1>
<p>Find me in app/views/posts/edit.html.erb</p>

<%= form_tag("/posts/#{@post.id}", method: :put) do%>
  제목 : <%= text_field_tag("post[title]", @post.title) %> <br>
  내용 : <%= text_area_tag("post[content]", @post.content, size: "25x10") %> <br>
  <%#= file_field_tag("avatar") %> <br>
  <%= submit_tag("글 수정") %>
<% end %>
