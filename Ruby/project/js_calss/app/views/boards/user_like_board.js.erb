$('.like').toggleClass('btn-outline-primary btn-primary');

var like_count = parseInt($('#like-count').text());

if(<%= @like.frozen? %>) {
  $('#like-count').text(like_count - 1);
} else {
  $('#like-count').text(like_count + 1);
}
