<script>
	var person = {
		name : '윤인성',
		eat : function(food) {
			alert(this.name + '이 ' + food + '을/를 먹습니다.');
		}
	};
	// 메서드를 호출합니다.
	person.eat('밥');
	//속성 추가 삭제 가능
	person.age: 10;
	alert(person.age);
</script>
