<script>
var i= 0;
var array = ['가', '나', '다'];
array[9] = 1000;
while(i<array.length){
	console.log(i + '번째 출력: '+array[i]);
	i++
	
	// 변수를 선얶합니다.
	var output = 0;
	// 반복을 수행합니다.
	for (var i = 0; i <= 100; i++) {
	output += i;
	}
	// 출력합니다.
	console.log(output);
}
</script>
