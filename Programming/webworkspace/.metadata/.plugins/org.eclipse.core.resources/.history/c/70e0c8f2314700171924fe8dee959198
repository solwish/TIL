<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style></style>
<script>
	//함수를 선얶합니다.
	function callTenTimes(callback) {
		// 10회 반복합니다.
		for (var i = 0; i < 10; i++) {
			// 매개 변수로 젂달된 함수를 호출합니다.
			callback();
		}
	}
	// 변수를 선얶합니다.
	var callback = function() {
		console.log('함수 호출');
	};
	// 함수를 호출합니다.
	callTenTimes(callback);
</script>
</head>
<body>

</body>
</html>